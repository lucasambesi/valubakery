<MudContainer MaxWidth="MaxWidth.False" Class="px-2 pt-3 pb-6">
    @for (var i = 0; i < 15; i++)
    {
        <MudPaper Elevation="1" Class="pa-3 mb-2" Style="border-radius:20px;">
            <MudStack Row AlignItems="AlignItems.Center" Spacing="2">
                <MudSkeleton SkeletonType="SkeletonType.Circle" Width="35px" Height="35px" />
                <MudStack Spacing="0" Class="w-100">
                    <MudSkeleton SkeletonType="SkeletonType.Text" Width="85%" Height="16px" />
                    <MudSkeleton SkeletonType="SkeletonType.Text" Width="65%" Height="14px" Class="mt-1" />
                </MudStack>
            </MudStack>
        </MudPaper>
    }
</MudContainer>
